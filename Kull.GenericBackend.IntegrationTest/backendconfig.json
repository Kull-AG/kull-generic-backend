{
  "$schema": "../backendconfig.schema.json",
  "Entities": {
    "Pet": {
      "GET": "spGetPets",
      "PUT": "spUpdatePet"
    },
    "Pet/{petid}": {
      "GET": "spGetPet",
      "POST": "spAddPet",
      "DELETE": "spDeletePet"
    },
    "Pet/search": {
      "GET": {
        "Sp": "spSearchPets"
      }
    },
    "Test": {
      "POST": {
        "SP": "spTestBackend",
        "OperationId": "GetBackend"
      },
      "Patch": {
        "SP": "spTestNoColumnName"
      }
    },
    "Date": {
      "Get": "spTestDate"
    },
    "Bug": {
      "Get": "spBuggyProc"
    },
    "Confidential": {
      "Get": "spUserNotPermitted"
    },
    "File": {
      "POST": {
        "SP": "spFile",
        "ResultType": "File"
      }
    }
  }
}
